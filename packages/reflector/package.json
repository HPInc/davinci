	"dependencies": {
		"reflect-metadata": "0.1.13"
	},
	"devDependencies": {
		"@types/mocha": "^5.2.6",
		"tslib": "^1.9.3",
		"tslint": "^5.16.0",
		"typescript": "^3.4.5"
	}
}
