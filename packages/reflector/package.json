{
	"name": "@davinci/reflector",
	"version": "0.7.0",
	"description": "Utility module used for metadata reflection",
	"main": "build/index.js",
	"types": "build/index.d.ts",
	"scripts": {
		"build": "tsc -p ./tsconfig.release.json",
		"tslint": "tslint -t stylish --project \"tsconfig.json\" '{src,test}/**/*{.ts,.tsx}'",
