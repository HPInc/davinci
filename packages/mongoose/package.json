	"peerDependencies": {
		"@davinci/core": "^1.0.0",
		"mongoose": "^5.11.0"
	},
	"optionalDependencies": {
		"@davinci/core": "^1.3.0"
	},
	"devDependencies": {
		"@davinci/core": "^1.0.0",
		"mongoose": "^5.11.0",
		"prettier": "1.19.1",
		"tslib": "^1.9.3",
		"typescript": "^3.7.5"
	}
