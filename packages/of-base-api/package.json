    "bluebird": "3.5.0",
    "debug": "3.1.0",
    "express": "4.16.1",
    "lodash": "4.17.11",
    "reflect-metadata": "0.1.13",
    "require-dir": "0.3.2"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.25",
    "@types/express": "4.16.1",
    "@types/lodash": "4.14.120",
    "@types/mocha": "5.2.5",
    "@types/mongoose": "5.3.12",
    "@types/node": "11.9.0",
    "eslint": "3.13.1",
    "eslint-config-airbnb-base": "11.0.1",
    "eslint-plugin-import": "2.2.0",
    "feathers-errors": "^2.9.2",
