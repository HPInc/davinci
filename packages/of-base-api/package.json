{
  "name": "@oneflow/of-base-api",
  "version": "0.0.28",
  "description": "Framework for rapid and consistent API development",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "example": "ts-node examples/crm/index.js",
    "start": "ts-node ./src/app.ts",
    "build": "tsc -p ./tsconfig.release.json",
    "start-release": "node build/src/app.js",
    "tslint": "tslint -t stylish --project \"tsconfig.json\" '{src,test}/**/*{.ts,.tsx}'",
    "tslintfix": "tslint --fix -c ./tslint.json '{src,test}/**/*{.ts,.tsx}'",
    "pretest": "npm run tslint && npm run build",
    "test": "mocha --exit",
    "cover": "nyc npm test"
  },
  "author": "Nigel Watson <nigel.watson@oneflowcloud.com> (http://www.oneflowsystems.com/)",
  "license": "ISC",
