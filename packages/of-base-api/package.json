    "mongoose": "5.4.13",
    "nyc": "13.1.0",
    "should": "11.1.2",
    "sinon": "4.5.0",
    "ts-node": "8.0.2",
    "tslint": "5.12.1",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.3.4000",
    "typings": "2.1.1"
  },
  "peerDependencies": {
    "mongoose": "5.4.13"
  }
