{
	"extends": "airbnb",
	"rules": {
	  "no-console": "off",
	  "comma-dangle": [2, "never"],
	  "arrow-parens": [2, "as-needed"],
	  "linebreak-style": 0,
	  "padded-blocks": 0,
	  "indent": [1, "tab"],
	  "no-underscore-dangle": [0],
	  "no-param-reassign": [2, { "props": false }],
	  "new-cap": [2, { "capIsNewExceptions": ["Router", "ObjectId"] }],
	  "no-tabs": 0,
	  "max-len": [1, 150], // extended
	  "no-plusplus": 0, // disabled
	  "generator-star-spacing": 0 // disabled as doesn't handle static generator methods
	},
	"env": {
	  "node": true
	}
  }
