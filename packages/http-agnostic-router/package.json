	},
	"peerDependencies": {
		"express": "^4.0.0",
		"itty-router": "^2.6.6"
	},
	"devDependencies": {
		"@davinci/core": "^2.0.0-next.17",
		"@types/itty-router-extras": "^0.4.0",
		"axios": "0.26.1",
		"itty-router": "^2.6.6",
		"supertest": "6.2.2",
		"typescript": "^4.6.2"
