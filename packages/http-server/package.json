{
	"name": "@davinci/http-server",
	"version": "0.0.1",
	"description": "Framework for rapid and consistent API development",
	"main": "build-cjs/index.js",
	"module": "build-esm/index.js",
	"types": "build-cjs/index.d.ts",
	"sideEffects": false,
	"scripts": {
		"build": "npm run build-cj && npm run build-esm",
		"build-cj": "../../node_modules/typescript/bin/tsc -p tsconfig.cjs.release.json",
		"build-esm": "../../node_modules/typescript/bin/tsc -p tsconfig.esm.release.json",
		"eslint": "../../node_modules/eslint/bin/eslint.js --color --c .eslintrc 'src/**/*.ts'",
		"pretest": "npm run eslint && npm run build",
		"test": "exit 0; mocha --exit",
		"cover": "nyc npm test"
	},
	"author": "HP",
	"license": "MIT",
	"dependencies": {
		"@davinci/core": "^1.9.1",
		"@davinci/reflector": "^1.1.1"
	},
	"devDependencies": {
		"typescript": "^4.6.2"
	}
}
