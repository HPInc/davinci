{
	"name": "@davinci/http-server",
	"version": "3.5.1",
	"description": "Framework for rapid and consistent API development",
	"main": "build-cjs/index.js",
	"module": "build-esm/index.js",
	"types": "build-cjs/index.d.ts",
	"sideEffects": false,
	"publishConfig": {
		"access": "public"
