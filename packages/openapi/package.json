		"hyperscript": "^2.0.2",
		"openapi-types": "^12.0.0",
		"pino": "^7.11.0"
	},
	"peerDependencies": {
		"@davinci/http-server": "1.0.0-next.0"
	},
	"devDependencies": {
		"@davinci/http-server": "^1.0.0-next.1",
		"@davinci/http-server-express": "^1.0.0-next.1",
		"@davinci/http-server-fastify": "^1.0.0-next.1",
		"@types/hyperscript": "^0.0.4",
		"@types/swagger-ui-dist": "^3.30.1",
