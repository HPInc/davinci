		"hyperscript": "^2.0.2",
		"openapi-types": "^12.0.0"
	},
	"peerDependencies": {
		"pino": "^8.7.0"
	},
	"devDependencies": {
		"@davinci/http-server": "^3.2.0",
		"@davinci/http-server-express": "^2.2.6",
		"@davinci/http-server-fastify": "^2.1.9",
		"@types/hyperscript": "^0.0.4",
		"@types/swagger-ui-dist": "^3.30.1",
		"axios": "^1.2.0",
		"pino": "^8.7.0",
